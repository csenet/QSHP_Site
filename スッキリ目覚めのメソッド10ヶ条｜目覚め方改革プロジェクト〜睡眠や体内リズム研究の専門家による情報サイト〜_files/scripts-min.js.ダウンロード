$("img").lazyload(),$((function(){$('a[href^="#"]').click((function(){var e=$(this).attr("href"),o=$("#"==e||""==e?"html":e).offset().top;return $("html, body").animate({scrollTop:o},500,"swing"),!1}))})),$(document).ready((function(){$(".loader").fadeOut("fast");var e=$(".page .header, .other .header").height();$(".page #contents, .other #contents").css("padding-top",e),$(".check-btn thead").css("top",e)})),$(window).scroll((function(){$(window).scrollTop()>80?$(".header").addClass("fixed"):$(".header").removeClass("fixed")})),$((function(){var e;$(".trigger_menu").on("click",(function(){return e=$(window).scrollTop(),$("#modal_menu").fadeIn(),!1})),$(".closeBtn").on("click",(function(){return $("#modal_menu").fadeOut(),$("body,html").stop().animate({scrollTop:e},100),!1}))}));